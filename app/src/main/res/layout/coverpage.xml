<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent" android:layout_height="match_parent" >

    <LinearLayout android:id="@+id/cover"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <RelativeLayout android:id="@+id/content_container"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@drawable/_bg__cover_ek2_shape">

            <RelativeLayout android:id="@+id/page_cover_ek1"
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <TextView android:id="@+id/start_tracking"
                    android:textStyle="bold"
                    android:layout_gravity="center_horizontal|center"
                    android:gravity="center"
                    android:textSize="22sp"
                    android:textColor="@color/start_tracking_color"
                    android:layout_marginTop="460dp"
                    android:layout_width="171dp"
                    android:layout_height="47dp"
                    android:text="@string/start_tracking_string"
                    android:layout_centerHorizontal="true"
                    />

                <TextView android:id="@+id/let_s_join_our_community_to_track_nutrients_in_your_food_"
                    android:textStyle="bold"
                    android:layout_gravity="center_horizontal|center"
                    android:gravity="center"
                    android:textSize="22sp"
                    android:textColor="@color/choice"
                    android:layout_marginTop="500dp"
                    android:layout_width="353dp"
                    android:layout_height="79dp"
                    android:text= "lets join our community to track nutrients in your food"
                    android:layout_centerHorizontal="true"
                    />

                <ImageView
                    android:id="@+id/rectangle_188"
                    android:layout_width="92dp"
                    android:layout_height="93dp"
                    android:layout_marginStart="145dp"
                    android:layout_marginLeft="0dp"
                    android:layout_marginTop="375dp"
                    android:contentDescription="rectangle_188"
                    android:src="@drawable/rectangle_188"
                    android:layout_centerHorizontal="true"/>

                <ImageView
                    android:id="@+id/ellipse_3"
                    android:layout_width="104dp"
                    android:layout_height="86dp"
                    android:layout_alignParentTop="true"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentRight="true"
                    android:layout_marginStart="8dp"
                    android:layout_marginLeft="0dp"
                    android:layout_marginTop="290dp"
                    android:layout_marginEnd="280dp"
                    android:layout_marginRight="256dp"
                    android:contentDescription="ellipse_3"
                    android:src="@drawable/ellipse_3" />

                <ImageView
                    android:id="@+id/ellipse_7"
                    android:layout_width="96dp"
                    android:layout_height="82dp"
                    android:layout_alignParentTop="true"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentRight="true"
                    android:layout_marginStart="283dp"
                    android:layout_marginLeft="0dp"
                    android:layout_marginTop="160dp"
                    android:layout_marginEnd="-17dp"
                    android:layout_marginRight="-17dp"
                    android:contentDescription="ellipse_7"
                    android:src="@drawable/ellipse_7" />

                <ImageView
                    android:id="@+id/ellipse_6"
                    android:layout_width="83dp"
                    android:layout_height="84dp"
                    android:layout_alignParentTop="true"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentRight="true"
                    android:layout_marginStart="261dp"
                    android:layout_marginLeft="0dp"
                    android:layout_marginTop="290dp"
                    android:layout_marginEnd="18dp"
                    android:layout_marginRight="18dp"
                    android:contentDescription="ellipse_6"
                    android:src="@drawable/ellipse_6" />

                <ImageView
                    android:id="@+id/image_1"
                    android:layout_width="111dp"
                    android:layout_height="104dp"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentTop="true"
                    android:layout_marginStart="40dp"
                    android:layout_marginLeft="40dp"
                    android:contentDescription="image_1"
                    android:src="@drawable/image_1" />

                <ImageView
                    android:id="@+id/image_3"
                    android:layout_width="122dp"
                    android:layout_height="124dp"
                    android:layout_alignParentTop="true"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentRight="true"
                    android:layout_marginStart="165dp"
                    android:layout_marginLeft="165dp"
                    android:layout_marginEnd="55dp"
                    android:layout_marginRight="55dp"
                    android:contentDescription="image_3"
                    android:src="@drawable/image_3" />

                <ImageView
                    android:id="@+id/image_4"
                    android:layout_width="112dp"
                    android:layout_height="106dp"
                    android:layout_alignParentTop="true"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentRight="true"
                    android:layout_marginStart="-120dp"
                    android:layout_marginLeft="0dp"
                    android:layout_marginTop="140dp"
                    android:layout_marginEnd="308dp"
                    android:layout_marginRight="297dp"
                    android:contentDescription="image_4"
                    android:src="@drawable/image_4"
                    />
                <TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
                    android:id="@+id/simpleTableLayout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_alignParentBottom="true"
                    android:stretchColumns="*">

                    <TableRow

                        android:id="@+id/firstRow"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content">

                        <Button android:id="@+id/signup"
                            android:background="#3CB371"
                            android:textStyle="bold"
                            android:textSize="22sp"
                            android:shadowRadius="4"
                            android:shadowColor="#000000"
                            android:shadowDx="0"
                            android:shadowDy="4"
                            android:textColor="@color/start_tracking_color"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:padding="18dip"
                            android:text="Signup"
                            />

                        <Button android:id="@+id/login"
                            android:textStyle="bold"
                            android:textSize="22sp"
                            android:shadowRadius="4"
                            android:shadowColor="#000000"
                            android:shadowDx="0"
                            android:shadowDy="4"
                            android:textColor="@color/login_color"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:background="#FFFFFF"
                            android:padding="18dip"
                            android:text="Login"
                            />

                    </TableRow>
                </TableLayout>

                <ImageView
                    android:id="@+id/ellipse_3_ek1"
                    android:layout_width="232.03dp"
                    android:layout_height="150.48dp"
                    android:layout_alignParentTop="true"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentRight="true"
                    android:layout_marginStart="0dp"
                    android:layout_marginLeft="0dp"
                    android:layout_marginTop="220dp"
                    android:layout_marginEnd="90dp"
                    android:layout_marginRight="161dp"
                    android:contentDescription="ellipse_3_ek1"
                    android:src="@drawable/ellipse_3_ek1"
                    android:layout_centerHorizontal="true"/>

                <ImageView
                    android:id="@+id/mask_group"
                    android:layout_width="250dp"
                    android:layout_height="250dp"
                    android:layout_alignParentTop="true"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentRight="true"
                    android:layout_marginStart="0dp"
                    android:layout_marginLeft="0dp"
                    android:layout_marginTop="122dp"
                    android:layout_marginEnd="75dp"
                    android:layout_marginRight="161dp"
                    android:contentDescription="mask_group"
                    android:src="@drawable/mask_group" />

            </RelativeLayout>

        </RelativeLayout>

    </LinearLayout>
</LinearLayout>