<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">

    <LinearLayout
        android:id="@+id/search_user"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <RelativeLayout
            android:id="@+id/content_container"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#ffffff">

            <RelativeLayout
                android:id="@+id/text_input___filled"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="0dp">

                <View android:id="@+id/overlay_ek4"
                    android:background="@drawable/overlay_ek4_shape"
                    android:alpha="0.4"
                    android:layout_marginTop="58dp"
                    android:layout_width="wrap_content"
                    android:layout_height="50dp"
                    />

                <View android:id="@+id/home_indicator_ek17"
                    android:background="@drawable/home_indicator_ek17_shape"
                    android:layout_marginTop="60dp"
                    android:layout_width="1.5dp"
                    android:layout_height="40.63dp"
                    android:layout_centerHorizontal="true"
                    />

                <Button android:id="@+id/users"
                    android:textStyle="bold"
                    android:layout_gravity="center_horizontal|center"
                    android:gravity="center"
                    android:textSize="17sp"
                    android:layout_marginLeft="30dp"
                    android:layout_marginTop="60dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@null"
                    android:text="User"
                    android:textColor="#ffffff"
                    android:shadowColor="#ffffff"
                    android:shadowDx="1"
                    android:shadowDy="2"
                    android:shadowRadius="6"
                    />

                <Button
                    android:id="@+id/recipes"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentRight="true"
                    android:layout_gravity="center_horizontal|center"
                    android:layout_marginLeft="170dp"
                    android:layout_marginTop="60dp"
                    android:layout_marginEnd="37dp"
                    android:layout_marginRight="67dp"
                    android:background="@null"
                    android:gravity="center"
                    android:text="Recipes"
                    android:textSize="16sp"
                    android:textStyle="bold" />

            </RelativeLayout>

            <ImageButton
                android:id="@+id/backk"
                android:layout_width="45dp"
                android:layout_height="45dp"
                android:layout_marginStart="15dp"
                android:layout_marginLeft="15dp"
                android:layout_marginTop="6dp"
                android:background="#ffffff"
                android:src="@drawable/ic_back" />

            <RelativeLayout
                android:id="@+id/close_circle_1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="290dp"
                android:layout_marginLeft="231.32dp"
                android:layout_marginTop="16dp">

                <RelativeLayout
                    android:id="@+id/layer_2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">
                </RelativeLayout>

            </RelativeLayout>

            <EditText
                android:id="@+id/search"
                android:layout_width="wrap_content"
                android:layout_height="38.5dp"
                android:layout_marginLeft="160dp"
                android:layout_marginTop="13dp"
                android:hint="Search "
                android:shadowColor="#ffffff"
                android:textColor="#000000"
                android:textSize="17sp"
                android:textStyle="bold" />

            <RelativeLayout
                android:id="@+id/group_2_ek11"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="90dp"
                android:layout_marginTop="18dp">

                <Button
                    android:id="@+id/ellipse_ek22"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:background="@drawable/ellipse_ek22_shape" />

                <View
                    android:id="@+id/rectangle_2_ek4"
                    android:layout_width="2.9dp"
                    android:layout_height="9.11dp"
                    android:layout_marginStart="15dp"
                    android:layout_marginLeft="0dp"
                    android:layout_marginTop="13dp"
                    android:background="@drawable/rectangle_2_ek4_shape" />

            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/navigation_bar_ek2"
                android:layout_width="match_parent"
                android:layout_height="55dp"
                android:layout_alignParentBottom="true"
                android:layout_marginTop="800dp"
                android:layout_marginBottom="41dp"
                android:background="#ffffff">

                <RelativeLayout
                    android:id="@+id/iconly_bold_home_ek2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="40dp"
                    android:layout_marginLeft="26dp"
                    android:layout_marginTop="15dp">


                    <RelativeLayout
                        android:id="@+id/home_ek10"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="3.13dp"
                        android:layout_marginLeft="3.13dp"
                        android:layout_marginTop="2.5dp">

                        <ImageButton
                            android:id="@+id/home1"
                            android:layout_width="23.75dp"
                            android:layout_height="30dp"
                            android:contentDescription="home_ek11"
                            android:src="@drawable/home_ek11" />

                    </RelativeLayout>

                </RelativeLayout>


                <com.blogspot.atifsoftwares.circularimageview.CircularImageView
                    android:id="@+id/profile1"
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:layout_alignParentRight="true"
                    android:layout_marginTop="15dp"
                    android:layout_marginRight="30dp"
                    android:src="@drawable/ic_logout"
                    app:c_border="true"
                    app:c_border_color="@color/colorGray1"
                    app:c_border_width="1dp" />

            </RelativeLayout>
            </RelativeLayout>
    </LinearLayout>
</LinearLayout>